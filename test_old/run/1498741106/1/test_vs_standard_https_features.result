ltm default-node-monitor {
    rule none
}
ltm dns analytics global-settings { }
ltm dns cache global-settings { }
ltm global-settings connection { }
ltm global-settings general {
    share-single-mac vmw-compat
}
ltm global-settings traffic-control { }
ltm persistence global-settings { }
ltm pool test_vs_standard_https_features_pool_0 {
    app-service /Common/test_vs_standard_https_features.app/test_vs_standard_https_features
    members {
        /Common/10.0.0.10%0:http {
            address 10.0.0.10
            app-service /Common/test_vs_standard_https_features.app/test_vs_standard_https_features
            session monitor-enabled
            state checking
        }
        /Common/10.0.0.11%0:http {
            address 10.0.0.11
            app-service /Common/test_vs_standard_https_features.app/test_vs_standard_https_features
            session user-disabled
            state checking
        }
        /Common/10.0.0.12%0:http {
            address 10.0.0.12
            app-service /Common/test_vs_standard_https_features.app/test_vs_standard_https_features
            session user-disabled
            state checking
        }
    }
    monitor /Common/http 
}
ltm profile client-ssl test_vs_standard_https_features_clientssl {
    app-service /Common/test_vs_standard_https_features.app/test_vs_standard_https_features
    cert /Common/default.crt
    cert-key-chain {
        default_ca-bundle {
            app-service /Common/test_vs_standard_https_features.app/test_vs_standard_https_features
            cert /Common/default.crt
            chain /Common/ca-bundle.crt
            key /Common/default.key
        }
    }
    chain /Common/ca-bundle.crt
    ciphers TLSv1_2:TLSv1_1:!TLSv1_2+LOW:!TLSv1_1+LOW:!EXPORT:!MD5:!ADH:@STRENGTH
    inherit-certkeychain false
    key /Common/default.key
    passphrase none
}
ltm profile http test_vs_standard_https_features_profile_http {
    app-service /Common/test_vs_standard_https_features.app/test_vs_standard_https_features
    defaults-from /Common/http
    insert-xforwarded-for enabled
}
ltm rule hsts_irule {
    app-service /Common/test_vs_standard_https_features.app/test_vs_standard_https_features
    when HTTP_RESPONSE {
  HTTP::header insert Strict-Transport-Security "max-age=300; includeSubDomains; preload"
}
}
ltm rule hsts_redirect_irule {
    app-service /Common/test_vs_standard_https_features.app/test_vs_standard_https_features
    when HTTP_REQUEST {
  HTTP::respond 301 Location "https://[getfield [HTTP::host] ":" 1][HTTP::uri]"
}
}
ltm tacdb licenseddb /Common/licensed-tacdb {
    partition none
}
ltm virtual test_vs_standard_https_features_default_vs_redirect_80 {
    app-service /Common/test_vs_standard_https_features.app/test_vs_standard_https_features
    destination /Common/172.16.0.100%0:http
    fw-enforced-policy test_vs_standard_https_features_firewall
    ip-protocol tcp
    mask 255.255.255.255
    profiles {
        /Common/tcp-lan-optimized {
            context serverside
        }
        /Common/tcp-wan-optimized {
            context clientside
        }
        test_vs_standard_https_features_profile_http { }
    }
    rules {
        hsts_redirect_irule
    }
    source 0.0.0.0/0
    translate-address enabled
    translate-port enabled
    vs-index 106
}
ltm virtual test_vs_standard_https_vs {
    app-service /Common/test_vs_standard_https_features.app/test_vs_standard_https_features
    description "Standard HTTPS Virtual Server with auto-created SSL profiles and advanced options"
    destination /Common/172.16.0.100%0:https
    fallback-persistence /Common/source_addr
    fw-enforced-policy test_vs_standard_https_features_firewall
    ip-protocol tcp
    mask 255.255.255.255
    persist {
        /Common/cookie {
            default yes
        }
    }
    pool test_vs_standard_https_features_pool_0
    profiles {
        /Common/httpcompression { }
        /Common/oneconnect { }
        /Common/tcp-lan-optimized {
            context serverside
        }
        /Common/tcp-wan-optimized {
            context clientside
        }
        test_vs_standard_https_features_clientssl {
            context clientside
        }
        test_vs_standard_https_features_profile_http { }
    }
    rules {
        hsts_irule
    }
    source 0.0.0.0/0
    translate-address enabled
    translate-port enabled
    vs-index 105
}
