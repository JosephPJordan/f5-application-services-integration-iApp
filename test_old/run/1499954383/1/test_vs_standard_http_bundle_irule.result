ltm default-node-monitor {
    rule none
}
ltm dns analytics global-settings { }
ltm dns cache global-settings { }
ltm global-settings connection { }
ltm global-settings general {
    share-single-mac vmw-compat
}
ltm global-settings traffic-control { }
ltm persistence global-settings { }
ltm pool test_vs_standard_http_bundle_irule_pool_0 {
    app-service /Common/test_vs_standard_http_bundle_irule.app/test_vs_standard_http_bundle_irule
    members {
        /Common/10.0.0.10%0:http {
            address 10.0.0.10
            app-service /Common/test_vs_standard_http_bundle_irule.app/test_vs_standard_http_bundle_irule
            session monitor-enabled
            state checking
        }
        /Common/10.0.0.11%0:http {
            address 10.0.0.11
            app-service /Common/test_vs_standard_http_bundle_irule.app/test_vs_standard_http_bundle_irule
            session user-disabled
            state checking
        }
        /Common/10.0.0.12%0:http {
            address 10.0.0.12
            app-service /Common/test_vs_standard_http_bundle_irule.app/test_vs_standard_http_bundle_irule
            session user-disabled
            state checking
        }
    }
    monitor /Common/http 
}
ltm rule bundle1 {
    app-service /Common/test_vs_standard_http_bundle_irule.app/test_vs_standard_http_bundle_irule
    when HTTP_REQUEST {
log local0. "blah"
}
}
ltm rule bundle2 {
    app-service /Common/test_vs_standard_http_bundle_irule.app/test_vs_standard_http_bundle_irule
    when HTTP_REQUEST {
}
}
ltm virtual test_vs_standard_http_bundle_irule_vs {
    app-service /Common/test_vs_standard_http_bundle_irule.app/test_vs_standard_http_bundle_irule
    description "Standard HTTP Virtual Server with Bundled iRules"
    destination /Common/172.16.0.100%0:http
    fallback-persistence /Common/source_addr
    ip-protocol tcp
    mask 255.255.255.255
    persist {
        /Common/cookie {
            default yes
        }
    }
    pool test_vs_standard_http_bundle_irule_pool_0
    profiles {
        /Common/http { }
        /Common/httpcompression { }
        /Common/oneconnect { }
        /Common/tcp-lan-optimized {
            context serverside
        }
        /Common/tcp-wan-optimized {
            context clientside
        }
    }
    rules {
        bundle2
        bundle1
    }
    source 0.0.0.0/0
    vs-index 18
}
