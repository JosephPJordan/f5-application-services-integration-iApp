ltm default-node-monitor {
    rule none
}
ltm dns analytics global-settings { }
ltm dns cache global-settings { }
ltm global-settings connection { }
ltm global-settings general {
    share-single-mac vmw-compat
}
ltm global-settings traffic-control { }
ltm persistence global-settings { }
ltm pool test_vs_standard_http_pool_0 {
    app-service /Common/test_vs_standard_http.app/test_vs_standard_http
    members {
        /Common/10.0.0.10%0:http {
            address 10.0.0.10
            app-service /Common/test_vs_standard_http.app/test_vs_standard_http
            session monitor-enabled
            state checking
        }
        /Common/10.0.0.11%0:http {
            address 10.0.0.11
            app-service /Common/test_vs_standard_http.app/test_vs_standard_http
            session user-disabled
            state checking
        }
        /Common/10.0.0.12%0:http {
            address 10.0.0.12
            app-service /Common/test_vs_standard_http.app/test_vs_standard_http
            session user-disabled
            state checking
        }
    }
    monitor /Common/http 
}
ltm virtual test_vs_standard_http_vs {
    app-service /Common/test_vs_standard_http.app/test_vs_standard_http
    description "Standard HTTP Virtual Server"
    destination /Common/172.16.0.100%0:http
    fallback-persistence /Common/source_addr
    ip-protocol tcp
    mask 255.255.255.255
    persist {
        /Common/cookie {
            default yes
        }
    }
    pool test_vs_standard_http_pool_0
    profiles {
        /Common/http { }
        /Common/httpcompression { }
        /Common/oneconnect { }
        /Common/tcp-lan-optimized {
            context serverside
        }
        /Common/tcp-wan-optimized {
            context clientside
        }
    }
    source 0.0.0.0/0
    vs-index 15
}
