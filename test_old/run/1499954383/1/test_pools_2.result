ltm default-node-monitor {
    rule none
}
ltm dns analytics global-settings { }
ltm dns cache global-settings { }
ltm global-settings connection { }
ltm global-settings general {
    share-single-mac vmw-compat
}
ltm global-settings traffic-control { }
ltm persistence global-settings { }
ltm pool adv_options_2 {
    allow-nat no
    app-service /Common/test_pools.app/test_pools
    description "pool 2 description"
    ip-tos-to-client mimic
    monitor min 2 of { /Common/tcp /Common/http }
    slow-ramp-time 300
}
ltm pool custom_pool_name_1 {
    app-service /Common/test_pools.app/test_pools
    description "pool 1 description"
    load-balancing-mode least-connections-member
    min-active-members 2
    monitor /Common/tcp and /Common/http 
}
ltm pool test_pools_pool_0 {
    app-service /Common/test_pools.app/test_pools
    members {
        /Common/10.0.0.10%0:http {
            address 10.0.0.10
            app-service /Common/test_pools.app/test_pools
            session user-disabled
            state down
        }
        /Common/10.0.0.11%0:http {
            address 10.0.0.11
            app-service /Common/test_pools.app/test_pools
            connection-limit 2000
            session user-disabled
            state user-down
        }
        /Common/10.0.0.12%0:http {
            address 10.0.0.12
            app-service /Common/test_pools.app/test_pools
            connection-limit 3000
            session user-disabled
            state down
        }
        /Common/2001:dead:beef:2::10%0.http {
            address 2001:dead:beef:2::10
            app-service /Common/test_pools.app/test_pools
            connection-limit 1000
            session monitor-enabled
            state down
        }
    }
    monitor /Common/tcp 
}
